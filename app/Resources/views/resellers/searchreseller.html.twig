<div class="row top-buffer">
	<div class="col-sm-8">
		<table class="table table-striped table-hover">
			<thead>
				<tr>
					<th><b>Reseller ID</b></th>
					<th><b>Company</b></th>
					<th><b>Type</b></th>

					<th>&nbsp;</th>
				</tr>
			</thead>
			<tbody>
				{% for r in data %}
				<tr>
					<td>{{r.resellerID}}</td>
					<td>{{r.company}}</td>
					<td>{{r.type}}</td>
					<td>
						<form action="{{path('addresellertores')}}" method="post" style="display:inline">
						<input type="hidden" name="reservationID" value="{{reservationID}}">
						<input type="hidden" name="resellerID" value="{{r.resellerID}}">
						<input type="submit" value="Select Reseller" class="btn btn-primary">
						</form>
					</td>
				</tr>
				{% endfor %}
			</tbody>
	</div>
</div>