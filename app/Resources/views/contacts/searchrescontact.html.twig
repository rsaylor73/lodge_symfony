<div class="row top-buffer">
	<div class="col-sm-8">
		<table class="table table-striped table-hover">
			<thead>
				<tr>
					<th><b>Name</b></th>
					<th><b>City</b></th>
					<th><b>Zip</b></th>
					<th><b>Country</b></th>
					<th>&nbsp;</th>
				</tr>
			</thead>
			<tbody>
				{% for d in data %}
				<tr>
					<td>{{d.first}} {{d.middle}} {{d.last}}</td>
					<td>{{d.city}}</td>
					<td>{{d.zip}}</td>
					<td>{{d.country}}</td>
					<td>
						<form action="{{path('addrespaxtores')}}" method="post" style="display:inline">
						<input type="hidden" name="reservationID" value="{{reservationID}}">
						<input type="hidden" name="contactID" value="{{d.contactID}}">
						<input type="submit" value="Select Contact" class="btn btn-primary">
						</form>
					</td>
				</tr>
				{% endfor %}
			</tbody>
	</div>
</div>
